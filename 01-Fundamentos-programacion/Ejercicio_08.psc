Algoritmo Ejercicio_08
	Definir grados Como Real
	
	Escribir "Ingrese el numero de grados"
	Leer  grados
	
	radianes <- grados*2*PI/360
	
	seno <- sen(grados)
	coseno <- cos(grados)
	tangente <- tan(grados)
	
	Imprimir "El seno es: ", seno
	Imprimir "El coseno es: ", coseno
	Imprimir "El tangente es: ", tangente
FinAlgoritmo
